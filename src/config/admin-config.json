              "placeholder": "Titre du jeu",
              "type": "text",
              "required": true
            },
            {
              "name": "shortname",
              "label": "Titre court",
              "placeholder": "Titre court du jeu",
              "type": "text",
              "required": true
            },
            {
              "name": "summary",
              "label": "Résumé",
              "placeholder": "Résumé du jeu",
              "type": "text",
              "required": true
            },
            {
              "name": "status",
              "label": "Etat de la session",
              "type": "select",
              "placeholder": false,
              "values": [
                {"id": 0, "title": "Inactive"},
                {"id": 1, "title": "Active"}
              ],
              "key": "id",
              "value": "title",
              "required": true,
              "defaultValue": -1
            }
          ]
        },
        "delete": {
          "labels": {
            "entity": "le jeu",
            "identifier": "title"
