[
      {
        "route": "/admin/games",
        "title": "Jeux",
        "client": "GameClient",
        "list": {
          "attributes": [
            {
              "name": "picture",
              "type": "image",
              "label": "Image"
            },
            {
              "name": "id",
              "hidden": true
            },
            {
              "label": "Titre",
              "name": "title"
            }
          ],
          "actions": [
            "new",
            "edit",
            "delete"
          ]
        },
        "form": {
          "attributes": [
            {
              "name": "picture",
              "label": "Image",
              "type": "image-uploader"
